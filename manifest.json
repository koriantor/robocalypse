{
  "manifest_version": 2,

  "name": "Robocalypse",
  "description": "Tool for tweeting as different Roberts.",
  "version": "0.1.0",

  "background": {
    "scripts": ["eventPage.js"],
	"persistent": false
  }
  "content_scripts": [ 
	{
	  "matches": ["https://www.twitter.com/*"],
	  "css": [],
	  "js": []
	}
  ],
  "incognito": "split",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
  
}